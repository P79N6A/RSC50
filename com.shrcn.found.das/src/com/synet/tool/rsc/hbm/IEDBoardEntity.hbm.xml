<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.synet.tool.rsc.model.IEDBoardEntity" table="IM103_Ied_Board" schema="rscapp">
  		<id name="im103Code">
            <column name="IM103_CODE" sql-type="varchar" length="48" not-null="true"/>
        </id>
        <property name="devDesc">
            <column name="DEV_DESC" sql-type="varchar" length="48" not-null="true"/>
        </property>
        <property name="boardCode">
            <column name="BOARD_CODE" sql-type="varchar" length="96"/>
        </property>
        <property name="portCode">
            <column name="PORT_CODE" sql-type="varchar" length="96"/>
        </property>
        <property name="matched">
            <column name="MATCHED" sql-type="int"/>
        </property>
        <many-to-one name="fileInfoEntity" class="com.synet.tool.rsc.model.FileInfoEntity" lazy="false">
            <column name="IM100_CODE" not-null="true"/>
        </many-to-one>
  </class>
</hibernate-mapping>