<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.synet.tool.rsc.model.Tb1047BoardEntity" table="tb1047_board" schema="rscapp">
        <id name="f1047Code">
            <column name="F1047_CODE" sql-type="char" length="48" not-null="true"/>
        </id>
        <property name="f1047Slot">
            <column name="F1047_Slot" sql-type="char" length="24" not-null="true"/>
        </property>
        <property name="f1047Desc">
            <column name="F1047_Desc" sql-type="char" length="96"/>
        </property>
        <property name="f1047Type">
            <column name="F1047_Type" sql-type="char" length="48" not-null="true"/>
        </property>
        <many-to-one name="tb1046IedByF1046Code" class="com.synet.tool.rsc.model.Tb1046IedEntity">
            <column name="F1046_CODE" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>