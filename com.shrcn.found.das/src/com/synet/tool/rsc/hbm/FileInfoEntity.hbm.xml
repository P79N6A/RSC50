<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.synet.tool.rsc.model.FileInfoEntity" table="IM100_File_Info" schema="rscapp">
         <id name="im100Code">
            <column name="IM100_CODE" sql-type="varchar" length="48" not-null="true"/>
        </id>
        <property name="fileName">
            <column name="FILE_NAME" sql-type="varchar" length="48" not-null="true"/>
        </property>
        <property name="fileDesc">
            <column name="FILE_DESC" sql-type="varchar" length="96"/>
        </property>
        <property name="fileType">
            <column name="FILE_TYPE" sql-type="varchar" length="96"/>
        </property>
  </class>
</hibernate-mapping>