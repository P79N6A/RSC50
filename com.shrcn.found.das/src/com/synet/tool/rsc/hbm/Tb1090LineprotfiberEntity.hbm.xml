<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.synet.tool.rsc.model.Tb1090LineprotfiberEntity" table="tb1090_lineprotfiber" schema="rscapp"
          >
        <id name="f1090Code">
            <column name="F1090_CODE" sql-type="char" length="48" not-null="true"/>
        </id>
        <property name="f1090Desc">
            <column name="F1090_DESC" sql-type="char" length="96" not-null="true"/>
        </property>
        <property name="f1090FiberNo">
            <column name="F1090_FiberNo" sql-type="char" length="24"/>
        </property>
        <property name="f1090PortNo">
            <column name="F1090_PortNo" sql-type="char" length="24"/>
        </property>
        <many-to-one name="tb1046IedByF1046Code" class="com.synet.tool.rsc.model.Tb1046IedEntity" lazy="false">
            <column name="F1046_CODE" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>